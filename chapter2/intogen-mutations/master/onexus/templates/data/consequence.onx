        </field>
        <field>
            <label>pph2-trans-class</label>
            <title>Pph2 transfic class</title>
            <id>PPH2_TRANSFIC_CLASS</id>
            <type>string</type>
        </field>
        <field>
            <label>ma</label>
            <title>Ma</title>
            <id>MA_SCORE</id>
            <type>double</type>
        </field>
        <field>
            <label>ma-trans</label>
            <title>Ma transfic</title>
            <id>MA_TRANSFIC</id>
            <type>double</type>
        </field>
        <field>
            <label>ma-trans-class</label>
            <title>Ma transfic class</title>
            <id>MA_TRANSFIC_CLASS</id>
            <type>string</type>
        </field>
        <field>
            <label>ct</label>
            <title>Ct</title>
            <id>CT</id>
            <type>string</type>
        </field>
        <field>
            <label>chr</label>
            <title>Chromosome</title>
