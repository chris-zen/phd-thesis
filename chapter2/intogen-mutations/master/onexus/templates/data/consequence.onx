        <field>
            <label>start</label>
            <title>start</title>
            <id>START</id>
            <type>integer</type>
        </field>
        <field>
            <label>allele</label>
            <title>Allele</title>
            <id>ALLELE</id>
            <type>string</type>
        </field>
        <field>
            <label>protein</label>
            <title>Protein</title>
            <id>PROTEIN_ID</id>
            <type>string</type>
        </field>
        <field>
            <label>uniprot</label>
            <title>Uniprot</title>
            <id>UNIPROT_ID</id>
            <type>string</type>
        </field>
        <field>
            <label>protein-pos</label>
            <title>Protein position</title>
            <id>PROTEIN_POS</id>
            <type>string</type>
        </field>
        <field>
            <label>aa change</label>
            <title>Aa change</title>
            <id>AA_CHANGE</id>
            <type>string</type>
        </field>
