        <field>
            <label>gene</label>
            <title>Gene</title>
            <id>GENE_ID</id>
            <type>string</type>
        </field>
        <field>
            <label>transcript</label>
            <title>Transcript</title>
            <id>TRANSCRIPT_ID</id>
            <type>string</type>
        </field>
        <field>
            <label>sift</label>
            <title>Sift</title>
            <id>SIFT_SCORE</id>
            <type>double</type>
        </field>
        <field>
            <label>sift-trans</label>
            <title>Sift transfic</title>
            <id>SIFT_TRANSFIC</id>
            <type>double</type>
        </field>
        <field>
            <label>sift-trans-class</label>
            <title>Sift transfic class</title>
            <id>SIFT_TRANSFIC_CLASS</id>
            <type>string</type>
        </field>
        <field>
            <label>pph2</label>
            <title>Pph2</title>
            <id>PPH2_SCORE</id>
            <type>double</type>
        </field>
        <field>
            <label>pph2-trans</label>
            <title>Pph2 transfic</title>
            <id>PPH2_TRANSFIC</id>
